                        <!-- Телефон -->
                        <a href="tel:+74951202299" class="topcontent-header__nav-menu-link">
                            <span>+7 495 120-22-99</span>
                        </a>

                        <!-- Курс валют (можно захардкодить) -->
                        <div class="topcontent-header__currencies">
                            <span class="topcontent-header__currencies-names">USD / EUR</span>
                            <div class="topcontent-header__nav-submenu topcontent-header__nav-submenu_currencies">
                                <div class="topcontent-header__currencies-inner">
                                    <div class="topcontent-header__currencies-inner-header">USD</div>
                                    <div class="topcontent-header__currencies-inner-header">EUR</div>
                                    <div class="topcontent-header__currencies-inner-rate">84.44 ₽</div>
                                    <div class="topcontent-header__currencies-inner-rate">97.25 ₽</div>
                                </div>
                                <div class="topcontent-header__currencies-date">06.11.2025</div>
                                <div class="topcontent-header__currencies-date">
                                    <a href="/exchange-rates">Архив курсов</a>
                                </div>
                            </div>
                        </div>

                        <!-- ЛК -->
                        <a target="_blank"
                           href="https://online.onetouch.travel/cl_refer_person"
                           class="topcontent-header__cabinet-link">
                            <img class="topcontent-header__cabinet-img"
                                 src="/images/lk-user.svg" width="15" height="16" alt="">
                            Личный кабинет
                        </a>
                    </div>
                </div>

            </div>
        </div>
    </header>

    <!-- ====== КОНТЕНТ СТРАНИЦЫ ОТЕЛЯ ====== -->
    <div class="cmn-l-content cmn-l-content_main">

        <!-- Хлебные крошки -->
        <div class="cmn-l-breadcrumbs">
            <div class="cmn-l-breadcrumbs__items">
                <a class="cmn-l-breadcrumbs__item" href="/">Главная</a> »
                <a class="cmn-l-breadcrumbs__item" href="/countries/">Страны</a> »
                <a class="cmn-l-breadcrumbs__item" href="/country/$CountrySlug">$CountryName</a> »
                <a class="cmn-l-breadcrumbs__item" href="/country/$CountrySlug/hotels/">Список отелей</a> »
                <span class="cmn-l-breadcrumbs__item">$HotelName</span>
            </div>
        </div>

        <div class="cmn-l-block-wrap">

            <!-- Заголовок: Название + звезды -->
            <h2 class="cmn-l-block-title">
                ${HotelName} <span class="hotel-stars">$StarsHtml</span>
            </h2>

            <!-- Локация под заголовком -->
            <div class="hotel-location-line">
                <a href="/country/$CountrySlug/" class="hotel-location-country">$CountryName</a>,
                <span class="hotel-location-city">$City</span>
                <span class="hotel-location-district">$District</span>
            </div>

            <!-- Табы (Описание / Номера / Инфраструктура / Акции) -->
            <div class="pages-main-nav">
                <a class="pages-main-nav__item pages-main-nav__item_active">Описание</a>
                <a class="pages-main-nav__item">Номера</a>
                <a class="pages-main-nav__item">Инфраструктура</a>
                <a class="pages-main-nav__item">Акции и новости</a>
            </div>

            <!-- Верхняя часть: большое фото + правая колонка с фотографиями -->
            <div class="hotel-top-block">

                <!-- Большое фото слева -->
                <div class="hotel-top-block__main-photo">
                    <img src="$MainPhotoUrl" alt="$HotelName" class="hotel-main-image">
                </div>

                <!-- Правая колонка: маленькие фото -->
                <div class="hotel-top-block__side-photos">
                    $SidePhotosHtml
                    <!--
                        Ожидаемый формат SidePhotosHtml:
                        <div class="hotel-thumb">
                            <img src="/images/hotels/xxx1.jpg" alt="">
                        </div>
                        <div class="hotel-thumb">
                            <img src="/images/hotels/xxx2.jpg" alt="">
                        </div>
                        ...
                    -->
                </div>
            </div>

            <!-- Можно добавить ссылку "На карте" где-нибудь справа -->
            <div class="hotel-map-link-row">
                <a href="$MapUrl" class="hotel-map-link">На карте</a>
            </div>

            <!-- Основной текстовой блок с описанием -->
            <div class="hotel-info-grid">

                <div class="hotel-info-row">
                    <div class="hotel-info-label">Расположение</div>
                    <div class="hotel-info-value">
                        $LocationHtml$
                    </div>
                </div>

                <div class="hotel-info-row">
                    <div class="hotel-info-label">Пляж</div>
                    <div class="hotel-info-value">
                        $BeachHtml
                    </div>
                </div>

                <div class="hotel-info-row">
                    <div class="hotel-info-label">Основная информация</div>
                    <div class="hotel-info-value">
                        $MainInfoHtml
                    </div>
                </div>

                <div class="hotel-info-row">
                    <div class="hotel-info-label">Контакты</div>
                    <div class="hotel-info-value">
                        $ContactsHtml
                        <!-- Пример:
                             Сайт: <a href="https://forshotel.com" target="_blank">forshotel.com</a><br>
                             Тел.: +90 (212) 517-09-61
                        -->
                    </div>
                </div>

                <div class="hotel-info-row">
                    <div class="hotel-info-label">Примечание</div>
                    <div class="hotel-info-value">
                        $NotesHtml
                    </div>
                </div>
            </div>

            <!-- Кнопка бронирования -->
            <div class="hotel-booking-row">
                <a href="/search/?hotelId=$HotelId"
                   class="hotel-booking-btn">
                    Забронировать от $MinPrice ₽
                </a>
            </div>

        </div> <!-- /cmn-l-block-wrap -->
    </div> <!-- /cmn-l-content_main -->

    <!-- ====== ФУТЕР ====== -->
